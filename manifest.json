{
    "manifest_version" : 3,
    "name" : "BOC Assistant",
    "version" : "1.0",
    "permissions": [
        "contextMenus",
        "activeTab",
        "storage",
        "contextMenus",
        "http://*/*"
    ],
    "dependencies": {
        "axios": "^1.6.2",
        "core-js": "^3.8.3",
        "element-ui": "^2.4.5",
        "less": "^4.2.0",
        "less-loader": "^11.1.3",
        "vue": "^2.6.14",
        "vue-router": "^3.5.1"
      },
    "description" : "The goole-plug assistant for BOC operation center!",
    "icons" : {
        "128" : "img/logo.png",
        "48" : "img/logo.png",
        "16" : "img/logo.png"
    },
    "action": {
        "default_icon": {
            "128" : "img/logo.png",
            "48" : "img/logo.png",
            "16" : "img/logo.png"
        },
        "default_popup": "popup.html"
    },
    "commands": {
        "rotate-image": {
            "suggested_key": {
                "default": "Ctrl+Shift+Y",
                "mac": "Command+Shift+Y"
            },
            "description": "Rotate Image"
        }
    },
    "background": {
        "service_worker": "js/background.js",
        "permissions":true
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["js/content.js"]
        }
    ]


}